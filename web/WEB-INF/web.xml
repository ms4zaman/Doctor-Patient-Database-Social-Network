<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ProfileViewServlet</servlet-name>
        <servlet-class>dochunt.profile.ProfileViewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddFriendshipServlet</servlet-name>
        <servlet-class>dochunt.friendship.AddFriendshipServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeeFriendshipServlet</servlet-name>
        <servlet-class>dochunt.friendship.SeeFriendshipServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>AddReviewServlet.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddReviewServlet</servlet-name>
        <servlet-class>dochunt.review.AddReviewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileSearchServlet</servlet-name>
        <servlet-class>dochunt.profile.ProfileSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileViewServlet</servlet-name>
        <url-pattern>/ProfileViewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddFriendshipServlet</servlet-name>
        <url-pattern>/AddFriendshipServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeeFriendshipServlet</servlet-name>
        <url-pattern>/SeeFriendshipServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddReviewServlet</servlet-name>
        <url-pattern>/AddReviewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileSearchServlet</servlet-name>
        <url-pattern>/ProfileSearchServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
